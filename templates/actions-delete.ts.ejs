import * as Types from '../../state/'
import * as <%=props.camelName%>Actions from './types'
import { <%=props.pascalName%> } from "../../../types/classes"
import * as <%=props.camelName%>sService from '../../../services/<%=props.camelName%>s.service'


export const delete<%=props.pascalName%> = (<%=props.camelName%>: <%=props.pascalName%>) => {
  return async (
    dispatch: Dispatch<Types.RootAction>
  ) => {
    const id = <%=props.camelName%>.id;
  try {
    await <%=props.camelName%>sService.remove(id);
    dispatch(<%=props.camelName%>Actions.Delete<%=props.pascalName%>(<%=props.camelName%>))
    } catch (e) {
     // console.log(e)
    }
  }
}
