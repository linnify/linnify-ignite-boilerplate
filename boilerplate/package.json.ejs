{
  "name": "ReactNativeLinnify",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && npm cache clean --force && npm i",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "test:watch": "jest --watch",
    "updateSnapshot": "jest --updateSnapshot",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:install": "cd android && ./gradlew assembleRelease && ./gradlew installRelease",
    "android:devices": "$ANDROID_HOME/platform-tools/adb devices",
    "android:logcat": "$ANDROID_HOME/platform-tools/adb logcat *:S ReactNative:V ReactNativeJS:V",
    "android:shake": "$ANDROID_HOME/platform-tools/adb devices | grep '\\t' | awk '{print $1}' | sed 's/\\s//g' | xargs -I {} $ANDROID_HOME/platform-tools/adb -s {} shell input keyevent 82",
    "lint": "tslint -p ./src",
    "lintdiff": "git diff --name-only --cached --relative | grep '\\.tsx?$' | xargs tslint | snazzy",
    "fixlint": "tslint --project ./src --fix",
    "git-hook": "npm run lint -s"
  },
  "dependencies": {
    "apisauce": "^1.0.0",
    "lodash": "^4.17.11",
    "ramda": "^0.25.0",
    "react-native": "0.57.3"
    "react-native-config": "^0.11.5",
    "react-navigation": "^2.18.0",
    "react-redux": "^5.0.5",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-persist": "^4.1.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "seamless-immutable": "^7.1.4",
    "typesafe-actions": "^2.0.4"
  },
  "devDependencies": {
    "@types/jest": "^21.1.4",
    "@types/lodash": "^4.14.117",
    "@types/ramda": "^0.25.39",
    "@types/react": "^16.4.16",
    "@types/react-native": "^0.57.6",
    "@types/react-navigation": "^2.0.24",
    "@types/react-redux": "^6.0.1",
    "@types/react-test-renderer": "^16.0.0",
    "@types/seamless-immutable": "^7.1.1",
    "prettier": "^1.12.1",
    "ts-jest": "^21.1.3",
    "tslint": "^5.7.0",
    "tslint-lines-between-class-members": "^1.2.4",
    "tslint-react": "^3.2.0",
    "typescript": "^2.9.1"
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  },
  "jest": {
    "preset": "react-native",
    "testMatch": [
      "**/tests/**/*.ts?(x)",
      "**/src/**/*Test.ts?(x)"
    ],
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/",
      "<rootDir>/lib/",
      "Tests/Setup"
    ],
    "setupFiles": [
      "./Tests/Setup.tsx"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json"
    ],
    "cacheDirectory": ".jest/cache"
  },
  "config": {}
}
